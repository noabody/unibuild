+        https://sourceforge.net/projects/libpng/files/libpng16/1.6.37/libpng-1.6.37.tar.xz
+        "$pkgbase".install)
+sha256sums=('SKIP'
+            '99c85a9b8e6c16131cefaf1040d5fa15cd565ecf8c71c1f644c8fa47fe1306dc'
+            '505e70834d35383537b6491e7ae8641f1a4bed1876dbfe361201fc80868d88ca'
+            'c00d324c7cf7a6d73300b3da8fdf74f729078c1e8f2275c905e136e34b20cd52')
+noextract=(libpng-1.6.37.tar.xz)
+
+pkgver() {
+  cd "$pkgbase"
+  local _ver="$(grep -Pio "vers.*\b.*\d+\.[[:alnum:]\.]+" imagine/include/imagine/config/version.h | grep -Pio '\d+\.[[:alnum:]\.]+')"
+  printf "%s.r%s.g%s" "$_ver" "$(git rev-list --count HEAD)" "$(git rev-parse --short HEAD)"
+}
+
+prepare() {
+  cp 0003-libarchive-Do-not-include-sys-mount.h-when-linux-fs..patch "$pkgbase"/imagine/bundle/all/src/libarchive/libarchive-sys-mount.h-linux-fs.patch
+  cp libpng-1.6.37.tar.xz "$pkgbase"/imagine/bundle/all/src/libpng
+  perl -pi -e 's|(\ttar \-mxJf.*)|\1\n\tpatch -d \$(libarchiveSrcDir) -p1 \< libarchive-sys-mount.h-linux-fs.patch|gi' "$pkgbase"/imagine/bundle/all/src/libarchive/common.mk
+  perl -pi -e 's|1.6.13|1.6.37|gi' "$pkgbase"/imagine/bundle/all/src/libpng/common.mk
+  ln -rsf "$pkgbase"/imagine/make/shortcut/common-builds/linux-x86_64-release-static.mk "$pkgbase"/EmuFramework/
+  ln -rsf "$pkgbase"/imagine/make/shortcut/common-builds/linux-x86_64-release.mk "$pkgbase"/C64.emu/
+  for _emu in 2600.emu C64.emu GBA.emu GBC.emu MD.emu MSX.emu NEO.emu NES.emu NGP.emu PCE.emu Snes9x; do
+  _sub="${_emu//./}"
+  _sub="${_sub,,}"
+  if [ ! -f "$_sub.svg" ]; then
+    convert -thumbnail 256x256 -strip "$pkgbase/$_emu"/res/icons/icon-144.png "$_sub".png
+    inkscape "$_sub".png --export-plain-svg --export-filename="$_sub".svg
+    rm "$_sub".png
+  fi
+  done
+  for _emu in . imagine EmuFramework 2600.emu C64.emu GBA.emu GBC.emu MD.emu MSX.emu NEO.emu NES.emu NGP.emu PCE.emu Snes9x; do
+    test ! -d "$pkgbase/$_emu/build" || rm -rf "$pkgbase/$_emu"/build
+    test ! -d "$pkgbase/$_emu/target" || rm -rf "$pkgbase/$_emu"/target
+  done
+  test ! -d "$pkgbase/temp" || rm -rf "$pkgbase"/temp
+  test ! -d "$pkgbase/imagine-sdk" || rm -rf "$pkgbase"/imagine-sdk
+}
+
+build() {
+  export IMAGINE_PATH="$startdir"/src/"$pkgbase"/imagine
+  export IMAGINE_SDK_PATH="$IMAGINE_PATH"-sdk
+  export PKG_CONFIG_PATH="$IMAGINE_SDK_PATH"/lib/pkgconfig
+  cd "$pkgbase"/imagine/bundle/all
+  bash makeAll-linux-x86_64-static.sh tempDir="$startdir"/src/"$pkgbase"/temp installDir="$IMAGINE_SDK_PATH" install
+  for _emu in imagine EmuFramework 2600.emu GBA.emu GBC.emu MD.emu MSX.emu NEO.emu NES.emu NGP.emu PCE.emu Snes9x; do
+    cd "$startdir"/src/"$pkgbase/$_emu"
+    (echo "$_emu" | grep -Pioq 'imagine|EmuFramework' && make -f linux-x86_64-release-static.mk -j4 install prefix="$IMAGINE_SDK_PATH" || make -f linux-x86_64-release-static.mk -j4)
+  done
+  cd ../C64.emu
+  make -f linux-x86_64-release.mk -j4 CFLAGS_OPTIMIZE_LEVEL_RELEASE_DEFAULT=-O1
+  cd ..
+}
