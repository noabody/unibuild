+++ b/emu-ex-plus-alpha/emu-ex-plus-alpha.install	1969-12-31 17:00:00.000000000 -0700
@@ -0,0 +1,21 @@
+# Colored makepkg-like functions
+msg_blue() {
+    printf "${blue}==>${bold} $1${all_off}\n"
+}
+
+note() {
+    printf "${blue}==>${yellow} NOTE:${bold} $1${all_off}\n"
+}
+
+all_off="$(tput sgr0)"
+bold="${all_off}$(tput bold)"
+blue="${bold}$(tput setaf 4)"
+yellow="${bold}$(tput setaf 3)"
+
+post_install() {
+    note "Ensure xrandr has a primary output:  xrandr | grep primary"
+    note "set with:  xrandr --output (output name) --primary"
+}
+post_upgrade() {
+    post_install
+}
